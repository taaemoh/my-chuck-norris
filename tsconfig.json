{
  "compilerOptions": {
      "jsx": "react",
      "esModuleInterop": true,
      "module": "commonjs",
      "target": "es5",
      "allowJs": true,
      "checkJs": false,
      "outDir": "./build",
      "removeComments": false,
      "noEmit": true,
      "pretty": true,
      "skipLibCheck": true,
      "strict": false,
      "moduleResolution": "node",
      "resolveJsonModule": true
      }
}